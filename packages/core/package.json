{
  "name": "@spz-loader/core",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "types": "./index.d.ts",
    "import": "./dist/index.js",
    "require": "./dist/index.umd.cjs"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:docker": "docker run --rm -v $(pwd)/lib/spz-wasm/:/src -u $(id -u):$(id -g) emscripten/emsdk ./build.sh",
    "check": "biome check ./lib ./src",
    "check:write": "biome check --write ./lib ./src"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.9.0",
    "typescript": "~5.6.3",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.3.0"
  }
}